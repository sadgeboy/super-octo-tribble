   "source": [
    "# One of the basic ideas in SymPy is that of a\n",
    "# Symbol object. If we want to tell Python that\n",
    "# treat \"x\" as \"x\", we create a Symbol object to\n",
    "# passing 'x' as an argument\n",
    "\n",
    "from sympy import Symbol\n",
    "\n",
    "x = Symbol('x')\n",
    "\n",
    "y = Symbol('x')\n",
    "\n",
    "print(x+y)"
   ]
  }
