    "# Next, we may want to \"evaluate\" the value of an algebraic expression\n",
    "\n",
    "from sympy import Symbol\n",
    "\n",
    "x = Symbol('x')\n",
    "\n",
    "expr = 2*x + 3*x**2\n",
    "\n",
    "# now evaluate expr using subs() method\n",
    "\n",
    "expr_val = expr.subs({x:1})\n",
    "\n",
    "print(expr_val)"
   ]
  }
